<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
	xmlns:gap="http://phonegap.com/ns/1.0"
	id="com.yourorg.testBuild" version="0.0.1">
	<name>Mobile app</name>
	<description>
		Videos for mobile
	</description>
	<preference name="orientation" value="landscape" />
	<author href="https://learntoprogram.tv/" email="mark@learntoprogram.tv">
		LearnToProgram.tv
	</author>
	
   <feature name="debug-server" required="true">
		<param name="domain" value="http://debug.custom.com"/>
   </feature>
	
	<icon src="img/icon.png" />
	<gap:splash src="splash.png" />
	
	<gap:splash src="img/android/splash/ldpi.png" gap:platform="android" gap:qualifier="ldpi" />
	<gap:splash src="img/android/splash/mdpi.png" gap:platform="android" gap:qualifier="mdpi" />
	<gap:splash src="img/android/splash/hdpi.png" gap:platform="android" gap:qualifier="hdpi" />
	<gap:splash src="img/android/splash/xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />
	
</widget>